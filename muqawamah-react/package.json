    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "framer-motion": "^10.16.16",
    "html-to-image": "^1.11.13",
    "multer": "^2.0.2",
    "node-fetch": "^3.3.2",
    "p5": "^2.1.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-p5": "^1.4.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
